@import B

Stuff blob
    @export aa u32 <- 5
    b data<u32, 8>
    c ptr<u32>
;

@export main fun
    v blob<Stuff>

    text blob<@B.String>

    text <- @B.StringForU32(v.aa)
    text.println()

    rep i u32 <- 0, i < 8, i <- i + 1
        text <- @B.StringForU32(v.b[i])
        text.println()
    ;

    text <- @B.StringForU32(v.c.vadr.u32)
    text.println()
;

