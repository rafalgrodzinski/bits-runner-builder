@export printNum fun: number u32
    biggest u64 <- 10
    rep biggest <= number: biggest <- biggest * 10
    biggest <- biggest / 10

    rep biggest > 0, biggest <- biggest / 10
        digit u64 <- number / biggest
        putchar(digit + '0')
        number <- number % biggest
    ;
;

@export main fun -> u64
    num u32 <- 9

        //if num > 10: printNum(4)
        //else if num > 8: printNum(5)

    if num > 5
        printNum(2)

        if num > 10: printNum(4)
        else if num > 8: printNum(5)
    else
        printNum(1)
    ;

    ret 0
;