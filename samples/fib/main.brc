@extern putchar fun: character u64 -> u32

fib fun: number u64 -> u64
    ret if number < 2
        number
    else
        fib(number - 1) + fib(number - 2)
    ;
;

printNum fun: number u64
    biggest u64 <- 10
    rep biggest <= number: biggest <- biggest * 10
    biggest <- biggest / 10

    rep biggest > 0, biggest <- biggest / 10
        digit u64 <- number / biggest
        putchar(digit + '0')
        number <- number % biggest
    ;
;

// Print first 20 fibonaci numbers
@export main fun -> u64
    rep i u64 <- 0, i < 20, i <- i + 1
        res u64 <- fib(i)
        printNum(res)
        putchar('\n')
    ;
    
    ret 0
;