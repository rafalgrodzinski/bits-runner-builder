@extern putchar fun: character u32 -> u32

getData fun -> data<u32, 7>
    hello data<u32> <- "Hello\n"
    helloCopy data<u32, 7> <- hello
    ret hello
;

@export main fun -> u32
    text data<u32, 3>
    text <- getData()
    text[2] <- 0

    rep i u32 <- 0, text[i] != 0:
        putchar(text[i])
        i <- i + 1
    ;

    ret 0
;