@extern putchar fun: character u64 -> u32

@export printNum fun: number u64
    biggest u64 <- 10
    rep biggest <= number: biggest <- biggest * 10
    biggest <- biggest / 10

    rep biggest > 0, biggest <- biggest / 10
        digit u64 <- number / biggest
        putchar(digit + '0')
        number <- number % biggest
    ;
;

@export tnum u64 <- 44
@export num u64 <- 77

@export main fun -> u32
    printNum(num)

    ret 0
;
