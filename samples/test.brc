@extern putchar fun: character u32 -> u32

print fun: text data<u32, 8>
    rep i u32 <- 0, text[i] != 0:
        putchar(text[i])
        i <- i + 1
    ;
;

@export main fun -> u32
    a bool <- false
    b bool <- false

    if a xor b: print("true\n\0\0\0") else print("false\n\0\0")

    ret 0
;