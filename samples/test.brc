@export main fun -> u32
    text data<u32> <- "Hello!!\n"

    rep i u32 <- 0, text[i] != 0:
        putchar(text[i])
        i <- i + 1
    ;

    newText data<u32> <- "Other\n"

    rep i u32 <- 0, newText[i] != 0:
        putchar(newText[i])
        i <- i + 1
    ;

    //newText <- text
    text <- newText

    rep i u32 <- 0, newText[i] != 0:
        putchar(newText[i])
        i <- i + 1
    ;

    rep i u32 <- 0, text[i] != 0:
        putchar(text[i])
        i <- i + 1
    ;

    text <- "Boo!\n"
    rep i u32 <- 0, text[i] != 0:
        putchar(text[i])
        i <- i + 1
    ;

    ret 0
;